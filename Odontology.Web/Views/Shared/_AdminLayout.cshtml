@inject SignInManager<ApplicationUser> signInManager
<!DOCTYPE html>
<html lang="en">
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-uWxY/CJNBR+1zjPWmfnSnVxwRheevXITnMqoEIeG1LJrdI0GlVs/9cVSyPYXdcSF" crossorigin="anonymous">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="~/css/site.css" rel="stylesheet" />
    <title>Dentmedic - @ViewBag.Title</title>
    @await RenderSectionAsync("Head", required: false)
</head>
<body class="site-content html">

    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" asp-controller="Home" asp-action="Index">Dentmedic</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-md-between" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a asp-controller="Article" asp-action="AdminList" class="nav-link">Naujienos</a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Authorization" asp-action="AdminList" class="nav-link">Naudotojai</a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Employee" asp-action="AdminList" class="nav-link">Darbuotojai</a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Visit" asp-action="AdminList" class="nav-link">Vizitai</a>
                    </li>
                </ul>
                <ul class="navbar-nav">
                    <li>
                        <a asp-controller="Home" asp-action="Index" class="nav-link">Grįžti į pagrindinį</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>

    <div>
        @RenderBody()
    </div>

<script src="https://kit.fontawesome.com/ba947858a4.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-kQtW33rZJAHjgefvhyyzcGF3C5TFyBQBA13V1RKPf4uH+bwyzQxZ6CmMZHmNBEfJ" crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

